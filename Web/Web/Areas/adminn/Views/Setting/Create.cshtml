@model Setting
@{
    ViewData["Title"] = "Create";
}

<div class="container">
    <div class="row">
        <div class="col-lg-6">
            <form asp-area="adminn" asp-controller="Setting" asp-action="create" model="post">
                <div class="mb-3">
                    <label asp-for="@Model.Logo" class="form-label">Logo</label>
                    <input class="form-control" asp-for="@Model.Logo">
                </div>
                <div class="mb-3">
                    <label asp-for="@Model.Content" class="form-label">Content</label>
                    <input class="form-control" asp-for="@Model.Content">
                </div>
                <div class="mb-3">
                    <label asp-for="@Model.Title" class="form-label">Title</label>
                    <input class="form-control" asp-for="@Model.Title">
                </div>
                <div class="mb-3">
                    <label asp-for="@Model.AboutTitle1" class="form-label">AboutTitle1</label>
                    <input class="form-control" asp-for="@Model.AboutTitle1">
                </div>
                <div class="mb-3">
                    <label asp-for="@Model.AboutTitle2" class="form-label">AboutTitle2</label>
                    <input class="form-control" asp-for="@Model.AboutTitle2">
                </div>
                <div class="mb-3">
                    <label asp-for="@Model.Adress" class="form-label">Adress</label>
                    <input class="form-control" asp-for="@Model.Adress">
                </div>
                <button type="submit" class="btn btn-primary">Create</button>
            </form>

        </div>
    </div>
</div>